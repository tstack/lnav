{
    "$schema": "https://lnav.org/schemas/config-v1.schema.json",
    "ui": {
        "theme-defs": {
            "grayscale": {
                "vars": {
                    "black": "#2d2a2e",
                    "red": "#f92772",
                    "green": "#a7e22e",
                    "yellow": "#fe9720",
                    "blue": "#5394ec",
                    "magenta": "#ae81ff",
                    "cyan": "#66d9ee",
                    "white": "#f6f6f6",
                    "plaintext": "#ccc"
                },
                "styles": {
                    "identifier": {
                        "background-color": "",
                        "color": "",
                        "bold": true
                    },
                    "text": {
                        "color": "$white",
                        "background-color": "$black"
                    },
                    "fuzzy-match": {
                        "underline": true,
                        "bold": true
                    },
                    "alt-text": {
                        "bold": true
                    },
                    "ok": {
                        "color": "$green",
                        "bold": true
                    },
                    "info": {
                        "color": "#aaa",
                        "bold": true
                    },
                    "error": {
                        "color": "$red",
                        "bold": true
                    },
                    "warning": {
                        "color": "$yellow",
                        "bold": true
                    },
                    "hidden": {
                        "color": "$yellow",
                        "bold": true
                    },
                    "cursor-line": {
                        "color": "#fff",
                        "background-color": "#555",
                        "bold": true
                    },
                    "disabled-cursor-line": {
                        "color": "#888",
                        "background-color": "#333"
                    },
                    "selected-text": {
                        "color": "#888"
                    },
                    "time-column": {
                        "background-color": "#444"
                    },
                    "adjusted-time": {
                        "color": "$magenta"
                    },
                    "skewed-time": {
                        "color": "$yellow"
                    },
                    "offset-time": {
                        "color": "$cyan"
                    },
                    "file-offset": {
                        "color": "#888"
                    },
                    "invalid-msg": {
                        "color": "$yellow"
                    },
                    "focused": {
                        "color": "$black",
                        "background-color": "$plaintext"
                    },
                    "disabled-focused": {
                        "color": "$plaintext",
                        "background-color": "#333"
                    },
                    "popup": {
                        "color": "$plaintext",
                        "background-color": "#424242"
                    },
                    "scrollbar": {
                        "color": "$black",
                        "background-color": "#888"
                    },
                    "h1": {
                        "bold": true,
                        "underline": true
                    },
                    "h2": {
                        "underline": true
                    },
                    "h3": {
                        "underline": true
                    },
                    "h4": {
                        "underline": true
                    },
                    "h5": {
                        "italic": true
                    },
                    "h6": {
                        "italic": true
                    },
                    "hr": {
                        "color": "#444"
                    },
                    "hyperlink": {
                        "underline": true
                    },
                    "list-glyph": {
                        "color": "#444"
                    },
                    "breadcrumb": {
                        "color": "#999",
                        "bold": true
                    },
                    "table-border": {
                        "color": "#444"
                    },
                    "table-header": {
                        "bold": true
                    },
                    "quote-border": {
                        "color": "#666",
                        "background-color": "#444"
                    },
                    "quoted-text": {
                        "background-color": "#444"
                    },
                    "footnote-border": {
                        "color": "#888",
                        "background-color": "#444"
                    },
                    "footnote-text": {
                        "color": "#eee",
                        "background-color": "#444"
                    },
                    "snippet-border": {
                        "color": "#888"
                    },
                    "indent-guide": {
                        "color": "#444"
                    }
                },
                "syntax-styles": {
                    "comment": {
                        "color": "#888"
                    },
                    "doc-directive": {
                        "color": "#aaa"
                    },
                    "null": {
                        "color": "#888"
                    },
                    "ascii-control": {
                        "color": "#aaa"
                    },
                    "non-ascii": {
                        "color": "#ccc"
                    },
                    "number": {
                        "bold": true
                    }
                },
                "status-styles": {
                    "disabled-title": {
                        "color": "#5394ec",
                        "background-color": "#353535",
                        "bold": true
                    },
                    "title": {
                        "color": "#f6f6f6",
                        "background-color": "#8a8a8a",
                        "bold": true
                    },
                    "subtitle": {
                        "color": "#e4e4e4",
                        "background-color": "#626262",
                        "bold": true
                    },
                    "title-hotkey": {
                        "color": "$black",
                        "background-color": "#5394ec",
                        "underline": true
                    },
                    "hotkey": {
                        "color": "#fff",
                        "underline": true
                    },
                    "text": {
                        "color": "#f6f6f6",
                        "background-color": "#353535"
                    },
                    "warn": {
                        "color": "$yellow",
                        "background-color": "#353535"
                    },
                    "alert": {
                        "color": "$red",
                        "background-color": "#353535"
                    },
                    "active": {
                        "color": "$green",
                        "background-color": "#353535"
                    },
                    "info": {
                        "color": "#555",
                        "background-color": "#2f2f2f"
                    },
                    "inactive": {
                        "color": "#555",
                        "background-color": "#2f2f2f"
                    },
                    "inactive-alert": {
                        "color": "$red",
                        "background-color": "#2f2f2f"
                    },
                    "suggestion": {
                        "color": "#888"
                    }
                },
                "log-level-styles": {
                    "warning": {
                        "color": "$yellow"
                    },
                    "error": {
                        "color": "$red"
                    },
                    "critical": {
                        "color": "$red"
                    },
                    "fatal": {
                        "color": "$red"
                    }
                }
            }
        }
    }
}