{
    "$schema": "https://lnav.org/schemas/format-v1.schema.json",
    "leveltest_log": {
        "description": "Log format used for testing levels",
        "regex": {
            "line": {
                "pattern": "^(?<timestamp>\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}) (?<level>[^ ]+) (?<body>.*)$"
            }
        },
        "level": {
            "trace": "trace",
            "debug": "debug",
            "debug2": "debug2",
            "debug3": "debug3",
            "info": "info",
            "warning": "warn",
            "fatal": "fatal"
        },
        "sample": [
            {
                "line": "2016-06-30 12:00:01 trace tracemessage"
            }
        ]
    }
}
