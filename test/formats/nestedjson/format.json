{
    "ntest_log" : {
        "title" : "Test JSON Log",
        "json" : true,
        "file-pattern" : "logfile_nested_json\\.json",
        "description" : "Test config",
        "line-format" : [
            { "field" : "ts" },
            " ",
            { "field" : "@fields/lvl" },
            " ",
            { "field" : "@fields/msg" }
        ],
        "level-field" : "@fields/lvl",
        "timestamp-field": "ts",
        "body-field" : "@fields/msg",
        "value" : {
            "@fields/user" : {
                "kind" : "string",
                "identifier" : true
            }
        }
    }
}
