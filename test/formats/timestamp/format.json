{
    "$schema": "https://lnav.org/schemas/format-v1.schema.json",
    "epoch_log": {
        "title": "epoch timestamp test",
        "regex": {
            "std": {
                "pattern": "^(?<timestamp>\\d+) (?<body>.*)$"
            },
            "non_epoch": {
                "pattern": "^(?<timestamp>\\d+-\\d+-\\d+ \\d+:\\d+:\\d+\\.\\d+) (?<body>.*)$"
            }
        },
        "timestamp-format" : [
            "%i",
            "%Y-%m-%d %H:%M:%S.%f"
        ],
        "sample": [
            {
                "line": "1428634687123 Hello, World!"
            }
        ]
    }
}
